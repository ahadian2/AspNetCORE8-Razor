@model IEnumerable<Razor.Models.Users>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    @foreach (var item in Model)
    {
        <div>
            <span>@item.Id</span>
            <span>@item.Name</span>
            <span>@item.Family</span>
        </div>
    }

</body>
</html>
